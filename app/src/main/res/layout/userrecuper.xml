<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundcorner">

    <RelativeLayout
        android:id="@+id/bar1"
        android:layout_width="wrap_content"
        android:layout_height="97dp"
        android:background="#80A7D27B"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">



        <ImageView
            android:id="@+id/image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/icone"
            android:paddingStart="12sp"
            android:paddingEnd="2dp"

            android:layout_marginStart="200dp"
            android:layout_marginTop="3dp"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Messages"
            android:textSize="40dp"
            android:textColor="@color/white"
            android:layout_marginStart="23dp"
            android:layout_marginTop="3dp"/>


        <!-- Rectangle "Chat" -->







        <!-- Rectangle "Status" -->


        <!--Rectangle calls-->



    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bar2"
        android:layout_width="410dp"
        android:layout_height="1040dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="100dp"
        android:background="@drawable/rounded_coin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/bar1"
        app:layout_constraintHorizontal_bias="0.425"
        app:layout_constraintStart_toStartOf="@id/bar1"
        app:layout_constraintTop_toBottomOf="@id/bar1"
        app:layout_constraintVertical_bias="0.497">
        <!--1 ere layout-->
        <RelativeLayout
            android:id="@+id/inter1"
            android:layout_width="350dp"
            android:layout_height="90dp"
            android:background="@drawable/rectangle"
            android:padding="16dp"
            android:layout_marginTop="40dp"
            android:layout_marginStart="20dp"
            android:onClick="onRelativeLayoutClicked"
            >


            <ImageView
                android:id="@+id/profile"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:background="@drawable/imageview"

                />

            <TextView
                android:id="@+id/firstname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:typeface="serif"
                android:layout_toEndOf="@id/profile"
                android:layout_marginStart="30dp"
                />

            <TextView
                android:id="@+id/phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/firstname"
                android:layout_marginStart="10dp"
                />

        </RelativeLayout>
        <!--2 ere layout-->
